<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="view_webhook_server_form">
            <field name="name">webhook.server.form</field>
            <field name="model">webhook.server</field>
            <field name="inherit_id" ref="juragan_webhook.view_webhook_server_form"/>
            <field name="arch" type="xml">
                <header>
                    <!-- <button class="oe_highlight" type="object" name="import_stock" string="Import Stock" />
                    <button class="oe_highlight" type="object" name="export_stock" string="Export Stock" /> -->
                </header>
                <page position="before">
                    <page string="Marketplace Account">
                        <h4>Tokopedia Accounts</h4>
                        <field name="mp_tokopedia_ids">
                            <tree editable="bottom">
                                <field name="name" readonly="1"/>
                                <field name="wh_id"/>
                            </tree>
                        </field>
                        <br/>
                        <h4>Shopee Accounts</h4>
                        <field name="mp_shopee_ids">
                            <tree editable="bottom">
                                <field name="name" readonly="1"/>
                                <field name="wh_id"/>
                            </tree>
                        </field>
                    </page>
                    <page string="Mapping">
                        <h4>Product Mapping</h4>
                        <field name="product_mapping_ids">
                            <tree editable="bottom" delete="0" create="0">
                                <field name="product_id" string="Product" domain="[('izi_id', '=', False)]"/>
                                <field string="Reference" name="reference" readonly="1"/>
                            </tree>
                        </field>
						<button type="object" name="get_product_mapping" string="Map Product" />
                    </page>
                </page>
            </field>
        </record>
    </data>
</odoo>