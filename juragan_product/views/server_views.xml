<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="view_webhook_server_form">
            <field name="name">webhook.server.form</field>
            <field name="model">webhook.server</field>
            <field name="inherit_id" ref="juragan_webhook.view_webhook_server_form"/>
            <field name="arch" type="xml">
                <header>
                    <!-- <button class="oe_highlight" type="object" name="import_stock" string="Import Stock" />
                    <button class="oe_highlight" type="object" name="export_stock" string="Export Stock" /> -->
                </header>
                <page position="before">
                    <page string="Data Mapping">
						<button type="object" name="start_warehouse_mapping" string="Start Warehouse Mapping" />
                        <field name="warehouse_mapping_ids">
                            <tree editable="bottom" delete="0" create="0">
                                <field name="warehouse_id" string="Warehouse"/>
                                <field name="reference" string="Reference" readonly="1"/>
                            </tree>
                        </field>
                        <br/>
						<button type="object" name="start_location_mapping" string="Start Location Mapping" />
                        <field name="location_mapping_ids">
                            <tree editable="bottom" delete="0" create="0">
                                <field name="location_id" string="Location" domain="[('usage', '=', 'internal')]"/>
                                <field name="reference" string="Reference" readonly="1"/>
                            </tree>
                        </field>
                        <br/>
						<button type="object" name="start_product_mapping" string="Start Product Mapping" />
                        <field name="product_mapping_ids">
                            <tree editable="bottom" delete="0" create="0">
                                <field name="product_id" string="Product" domain="[('type', '!=', 'service')]"/>
                                <field name="reference" string="Reference" readonly="1"/>
                            </tree>
                        </field>
                    </page>
                    <page string="Marketplace Account">
                        <h4>Tokopedia Accounts</h4>
                        <field name="mp_tokopedia_ids">
                            <tree editable="bottom">
                                <field name="name" readonly="1"/>
                                <field name="wh_id"/>
                            </tree>
                        </field>
                        <br/>
                        <h4>Shopee Accounts</h4>
                        <field name="mp_shopee_ids">
                            <tree editable="bottom">
                                <field name="name" readonly="1"/>
                                <field name="wh_id"/>
                            </tree>
                        </field>
                        <br/>
                        <h4>Lazada Accounts</h4>
                        <field name="mp_lazada_ids">
                            <tree editable="bottom">
                                <field name="name" readonly="1"/>
                                <field name="wh_id"/>
                            </tree>
                        </field>
                    </page>
                </page>
            </field>
        </record>
    </data>
</odoo>