<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="0">
    <record id="ir_cron_webhook_retry" model="ir.cron">
        <field name='name'>Webhook Retry</field>
        <field name='interval_number'>1</field>
        <field name='interval_type'>minutes</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="False"/>
        <field name="doall" eval="False"/>
        <field name="code">model.retry_requests()</field>
        <field name="state">code</field>
        <field name="model_id" ref="model_webhook_server_mq"/>
        <field name="active" eval="False"/>
    </record>

    <record id="cron_webhook_get_order" model="ir.cron">
        <field name="name">Webhook Get Order</field>
        <field name="model_id" ref="model_webhook_server"/>
        <field name="user_id" ref="base.user_admin"/>
        <field name="interval_number">1</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="code">model._run_get_orders()</field>
        <field name="active" eval="False"/>
    </record>
</odoo>
